{
	"name": "rhinocode",
	"displayName": "RhinoCode",
	"description": "RhinoCode interoperability extension",
	"publisher": "Robert McNeel & Associates",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/mcneel/rhinocodevscode"
	},
	"version": "0.0.1",
	"icon": "rhinocode.png",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Programming Languages",
		"Other"
	],
	"keywords": [
		"rhino3d",
		"rhinocode",
		"rhinoscript"
	],
	"activationEvents": [
		"onCommand:rhinocode.runinrhino"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rhinocode.runinrhino",
				"title": "Run In Rhino WIP"
			}
		],
		"configuration": {
			"title": "RhinoCode",
			"properties": {
				"rhinocode.rhinoInstallPath": {
					"type": "string",
					"default": "",
					"description": "Full install path of Rhino on your machine"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	}
}